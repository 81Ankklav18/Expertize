%p#notice= notice
%h1 Oracles
%table
  %thead
    %tr
      %th Name
      %th Email
      %th{:colspan => "3"}
  %tbody
    - @oracles.each do |oracle|
      %tr
        %td= oracle.name
        %td= oracle.email
        %td= link_to 'Show', oracle
        %td= link_to 'Edit', edit_oracle_path(oracle)
        %td= link_to 'Destroy', oracle, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Oracle', new_oracle_path
